   "source": [
    "def tower_builder(n_floors):\n",
    "    lis = []\n",
    "    for i in range (n_floors):\n",
    "        lis.append(\" \"*(n_floors-i-1) + \"*\"*(2*i+1) + \" \"*(n_floors-i-1))\n",
    "    return lis"
   ]
